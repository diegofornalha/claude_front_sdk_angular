{
  "name": "claude-front-sdk-angular-workspace",
  "version": "1.0.0",
  "description": "Angular SDK para integração com Claude RAG Backend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build claude-front-sdk",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:lib": "ng test claude-front-sdk",
    "test:lib:ci": "ng test claude-front-sdk --no-watch --code-coverage --browsers=ChromeHeadless",
    "test:coverage": "ng test claude-front-sdk --no-watch --code-coverage",
    "lint": "eslint . --ext .ts,.html",
    "lint:fix": "eslint . --ext .ts,.html --fix",
    "format": "prettier --write \"projects/**/*.{ts,html,scss,json}\" \"src/**/*.{ts,html,scss,json}\"",
    "format:check": "prettier --check \"projects/**/*.{ts,html,scss,json}\" \"src/**/*.{ts,html,scss,json}\"",
    "storybook": "ng run claude-front-sdk:storybook",
    "build-storybook": "ng run claude-front-sdk:build-storybook",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "docs": "typedoc --out docs/api projects/claude-front-sdk/src/public-api.ts",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/angular": "^8.6.15",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "^18.4.0",
    "eslint": "^9.17.0",
    "husky": "^9.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.0.0",
    "ng-packagr": "^18.2.0",
    "prettier": "^3.2.0",
    "storybook": "^8.6.15",
    "typedoc": "^0.28.15",
    "typescript": "~5.5.2",
    "typescript-eslint": "^8.18.0"
  },
  "lint-staged": {
    "projects/**/*.{ts,html}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{ts,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
